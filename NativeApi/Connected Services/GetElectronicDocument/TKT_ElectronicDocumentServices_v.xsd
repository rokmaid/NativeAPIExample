  <xsd:complexType name="Ticketing.Agent.ED">
    <xsd:annotation>
      <xsd:documentation>Agent information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="TicketingProvider" type="Code.TicketingProviderCode">
        <xsd:annotation>
          <xsd:documentation>Airline or Carrier Code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WorkLocation" type="Identifier.LocationExtended">
        <xsd:annotation>
          <xsd:documentation>The PCC where the agent issued the electronic document. This is normallly the agent's home PCC or authorized branch PCC.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="HomeLocation" type="Identifier.Location">
        <xsd:annotation>
          <xsd:documentation>3 digit all-alpha pseudo city code found in the requestor???s TJR under AA CITY and LOR CITY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IsoCountryCode" type="STL:CountryCode">
        <xsd:annotation>
          <xsd:documentation>The code which identifies the country of issuance as defined in IATA.                </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IataNumber" type="Identifier.Industry">
        <xsd:annotation>
          <xsd:documentation>An IATA assigned numeric code to identify the agent location authorized to issue   an electronic document.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CompanyName" type="STL:StringLength1to128">
        <xsd:annotation>
          <xsd:documentation>The name of the company where the agent belongs to. This comes from the agency's TJR. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CRSCode" type="STL:CRSCode">
        <xsd:annotation>
          <xsd:documentation>Abacus CRS CODE:7766	Infini CRS CODE:7884 Sabre CRS CODE:0011 </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="Code.AgentType">
      <xsd:annotation>
        <xsd:documentation>A code assigned to the type of agent who issued the transaction. 
                                 IATA Values can be:
A - Airline (AIR) ?????
T - Travel Agent (TVL)  </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sine" type="STL:AgentSine">
      <xsd:annotation>
        <xsd:documentation>The last 2 digits of the Agent sine who issued the document.                            </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Ticketing.Document.EMD.ED">
    <xsd:annotation>
      <xsd:documentation>EMD info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Indicators" type="TicketingDocument.Indicators.EMD">
        <xsd:annotation>
          <xsd:documentation>Ticketing document indicators with boolen values; default to 'false'
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Details" type="TicketingDocument.Details.EMD.ED">
        <xsd:annotation>
          <xsd:documentation>Details </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Customer" type="TicketingDocument.Customer.ED">
        <xsd:annotation>
          <xsd:documentation>Customer information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="16" name="Miscellaneous" type="TicketingDocument.Miscellaneous.ED">
        <xsd:annotation>
          <xsd:documentation>EMD details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Amounts" type="TicketingDocument.Amounts.EMD">
        <xsd:annotation>
          <xsd:documentation>Any amounts associated to the ticketing document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Taxes" type="TicketingDocument.Taxes.EMD">
        <xsd:annotation>
          <xsd:documentation>Tax amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" name="Payment" type="TicketingDocument.Payment.ED">
        <xsd:annotation>
          <xsd:documentation>Payment iinformation</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="99" name="Remark" type="TicketingDocument.Remark.EMD">
        <xsd:annotation>
          <xsd:documentation>Remarks such as endorsements</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RelatedDocument" type="TicketingDocument.Related">
        <xsd:annotation>
          <xsd:documentation>Related document info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ValueCalculation" type="TicketingDocument.ValueCalculation">
        <xsd:annotation>
          <xsd:documentation>Value calculation</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="Code.DocumentType" use="required">
      <xsd:annotation>
        <xsd:documentation>Ticketing document type issued by the agent.

                    Possible values can be:
                    TKT - Ticket
                    EMD - Electronic Miscellaneous Document</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="number" type="Identifier.TicketingDocument" use="required">
      <xsd:annotation>
        <xsd:documentation>The 13 digit electronic document number dispensed and assigned by the system upon issuance. Each EMD number can accomodate 1-4 service coupons. If more than 4 service coupons are required, additional 13 digit EMD numbers will be created by the system (in numeric sequence). When more than one EMD number is required, this is known as a conjuctive series. The maximum number of 13 digit EMD numbers in a conjuctive series is 4.  </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Ticketing.Document.Ticket.ED">
    <xsd:annotation>
      <xsd:documentation>TKT info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Details" type="TicketingDocument.Details.Ticket.ED">
        <xsd:annotation>
          <xsd:documentation>Detail information specific to an electronic ticket</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Customer" type="TicketingDocument.Customer.ED">
        <xsd:annotation>
          <xsd:documentation>Customer information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Affinity" type="TicketingDocument.Affinity.ED">
        <xsd:annotation>
          <xsd:documentation>Loyalty information such as frequent flyer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="16" name="ServiceCoupon" type="TicketingDocument.ServiceCoupon.Ticket.ED">
        <xsd:annotation>
          <xsd:documentation>An electronic ticket can have 1 to 16 coupons (including coniuctive series) </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RelatedDocument" type="TicketingDocument.Related.Ticket.ED">
        <xsd:annotation>
          <xsd:documentation>Other documents that are related to the primary document upon issuance
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Amounts" type="TicketingDocument.Amounts.Ticket.ED">
        <xsd:annotation>
          <xsd:documentation>Any amounts associated to the ticketing document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Taxes" type="TicketingDocument.Taxes.Ticket.ED">
        <xsd:annotation>
          <xsd:documentation>Any taxes paid by the customer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="3" name="Payment" type="TicketingDocument.Payment.ED">
        <xsd:annotation>
          <xsd:documentation>Two forms of payment are allowed. In case of exchanges, there could be another form of paymemt code 'ET' that signifies the use of travel documents to issue another ticketing    document.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Remark" type="TicketingDocument.Remark.ED">
        <xsd:annotation>
          <xsd:documentation>Remarks such as endorsements
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FareCalculation">
        <xsd:annotation>
          <xsd:documentation>Fare calculation text</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="New" type="FareCalculation.Details">
              <xsd:annotation>
                <xsd:documentation>Free text indicating fare calculation details</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="Code.DocumentType" use="required">
      <xsd:annotation>
        <xsd:documentation>Ticketing document type issued by the agent.

                    Possible values can be:
                    TKT - Ticket
                    EMD - Electronic Miscellaneous Document</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="number" type="Identifier.TicketingDocument" use="required">
      <xsd:annotation>
        <xsd:documentation>The 13 digit electronic document number dispensed and assigned by the system upon issuance. Each ticket can accomodate 1-4 flight coupons. If the passenger's itinerary requires more than 4 flight coupons, additional 13 digit document numbers will be created by the system (in numeric sequence). When more than one document number is required, this is known as a conjuctive series. The maximum number of 13 digit document numbers in a conjuctive series is 4. </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Ticketing.TransactionInfo.ED">
    <xsd:annotation>
      <xsd:documentation>Transaction info</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="STL:TransactionInfo">
        <xsd:sequence>
          <xsd:element name="LocalDateTime" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Date and time when the agent issued the ticketing transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SystemDateTime" type="xsd:dateTime">
            <xsd:annotation>
              <xsd:documentation>Date and time that the document was created in the ticketing system</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SystemProvider" type="Identifier.Provider">
            <xsd:annotation>
              <xsd:documentation>Global distribution system within the Sabre System or a carrier reservation system that issued a document.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="InputEntry" type="String.Message">
            <xsd:annotation>
              <xsd:documentation>Input entry initiated for this transaction that generates the ticketing document                   </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="sequence" type="xsd:integer">
          <xsd:annotation>
            <xsd:documentation>A number received from the host system that identifies the sequence that this transaction is sent. It is optional and only applies to the VCR where the system of record is PSS.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" type="Code.TicketingTransactionType">
          <xsd:annotation>
            <xsd:documentation>A code associated to the transaction type as received from PSS host system.
                            Values can be:
                            SALE
                            EXCH
                            RFND
                            ARMK
                            STAT
                            PNRA
                            VOID  </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Affinity.ED">
    <xsd:annotation>
      <xsd:documentation>Frequent flyer info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="FrequentFlyer" type="Affinity.FrequentFlyer.ED">
        <xsd:annotation>
          <xsd:documentation>Frequent Flyer information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Amounts.EMD">
    <xsd:annotation>
      <xsd:documentation>Amounts for EMD</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="New" type="Amounts.New">
        <xsd:annotation>
          <xsd:documentation>New amounts associated to the ticketing document                   </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Amounts.Ticket.ED">
    <xsd:annotation>
      <xsd:documentation>Amounts for ticket</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="New" type="Amounts.New.ED">
        <xsd:annotation>
          <xsd:documentation>Amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Other" type="Amounts.Other.ED">
        <xsd:annotation>
          <xsd:documentation>Other Amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amounts.Other.ED">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="AddCollect" type="Amount.Basic" />
      <xsd:element minOccurs="0" name="Commission" type="Amount.Commission" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.CurrencyConversion">
    <xsd:annotation>
      <xsd:documentation>Currency conversion</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="BankerSellingRate" type="CurrencyConversion.Details">
        <xsd:annotation>
          <xsd:documentation>Banker Selling rate</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BankerBuyingRate" type="CurrencyConversion.Details">
        <xsd:annotation>
          <xsd:documentation>Banker Buying Rate</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IataClearingHouseRate" type="CurrencyConversion.Details">
        <xsd:annotation>
          <xsd:documentation>Iata Clearing House rate</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Customer.ED">
    <xsd:annotation>
      <xsd:documentation>Customer information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Traveler" type="Customer.Traveler.Details.ED">
        <xsd:annotation>
          <xsd:documentation>A person who owns the document for travel and/or services</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Details.EMD.ED">
    <xsd:annotation>
      <xsd:documentation>EMD details</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="TourNumber" type="Identifier.Tour">
        <xsd:annotation>
          <xsd:documentation>A tour number (or tour code) is a 1-15 alphanumeric code which identifies a tour or was filed with the fare or was input by the agent to satisfy specific instructions from the airline.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Reservation" type="Reservation.Details.ED">
        <xsd:annotation>
          <xsd:documentation>Booking reference number provided by the airline or a GDS. This is also known as a Passenger Name Record (PNR).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LocalIssueDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The date and time when the EMD was issued in the agent's time zone as per the city code in the agency's TJR.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ValidatingProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>reserved for future use</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ItineraryType" type="Code.ItineraryType">
        <xsd:annotation>
          <xsd:documentation>1 digit indicator to specify domestic or international (D - domestic, I- international)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SettlementAuthorization" type="Identifier.TicketingSettlementAuthorization">
        <xsd:annotation>
          <xsd:documentation>A reference number generated by the Validating carrier authorising settlement of a coupon, usually present only for exchange and void transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ElectronicMiscType" type="Code.ElectronicMiscType">
        <xsd:annotation>
          <xsd:documentation>The type of the electronic miscellaneous document (A- Associated, S - Standalone)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ReasonForIssue" type="Code.TicketingDocumentReasonIssuanceCode_ATPCO_RFIC">
        <xsd:annotation>
          <xsd:documentation>A single character code associated to the reason for issuance as defined by IATA. Eg: D = financial impact  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InvoiceNumber" type="Number.Invoice">
        <xsd:annotation>
          <xsd:documentation>An agency assigned number to internally identify the associated invoice or combined itinerary/invoice document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AgencyBillingNumber" type="Number.AgencyBilling">
        <xsd:annotation>
          <xsd:documentation>illing reference number attached to the electronic document when it is issued by the agency. Also known as the Customer number or DK number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Details.Ticket.ED">
    <xsd:annotation>
      <xsd:documentation>TKT details</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="TourNumber" type="Identifier.Tour">
        <xsd:annotation>
          <xsd:documentation>A tour number (or tour code) is a 1-15 alphanumeric code which identifies a tour or was filed with the fare or was input by the agent to satisfy specific instructions from the airline.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Reservation" type="Reservation.Details.ED">
        <xsd:annotation>
          <xsd:documentation>6 digit booking reference number provided by the airline or a GDS. This is also known as a Passenger Name Record (PNR).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LocalIssueDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The date and time when the ticketing document was issued in the agent's time zone as per the city code in the agency's TJR.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AgencyBillingNumber" type="Number.AgencyBilling">
        <xsd:annotation>
          <xsd:documentation>Billing reference number attached to the electronic document when it is issued by the agency. Also known as the Customer number or DK number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="InvoiceNumber" type="Number.Invoice">
        <xsd:annotation>
          <xsd:documentation>An agency assigned number to internally identify the associated invoice or combined itinerary/invoice document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FareCalculationPricing" type="Code.FareCalculationPricing">
        <xsd:annotation>
          <xsd:documentation>A single digit numeric identifier reflecting the method of pricing for this transaction as entered on the electronic ticket.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.History.ED">
    <xsd:annotation>
      <xsd:documentation>Document history details</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="9999" name="Text" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>Reserved for future use</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Indicators.EMD">
    <xsd:annotation>
      <xsd:documentation>EMD indicators</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="commisionable" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A Y/N indicator to allow or disallow commission.           </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="electronic" type="xsd:boolean" use="required">
      <xsd:annotation>
        <xsd:documentation>A boolean (Y/N) indicator which identifies whether the document is produced electronically.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="interlineable" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A Y/N value representing that the document can be shared with another carrier.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="refundable" type="Code.IATAIndicators">
      <xsd:annotation>
        <xsd:documentation>A Y/N indicator representing whether the document is refundable.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="historical" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>reserved for future use</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Miscellaneous.ED">
    <xsd:annotation>
      <xsd:documentation>Miscellaneous info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Fee" type="Fee.Details" />
      <xsd:element minOccurs="0" name="OptionalService" type="OptionalService">
        <xsd:annotation>
          <xsd:documentation>Reason for issuance sub-code  (RFISC) as published in ATPCO.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CouponDetails" type="TicketingDocument.ServiceCoupon.EMD.ED">
        <xsd:annotation>
          <xsd:documentation>Each EMD service coupon contains all the infromation needed for the airline to deliver the services for that coupon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="99" name="Tax" type="Tax.Misc">
        <xsd:annotation>
          <xsd:documentation>Taxes info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AssociatedTicketNumber" type="Number.AssociatedDocument">
        <xsd:annotation>
          <xsd:documentation>Fee can be associated to a ticket number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Affinity" type="TicketingDocument.Affinity.ED">
        <xsd:annotation>
          <xsd:documentation>Frequent flyer information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Fee.Details">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Base" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>The base fee amount associated to the item or service which does not include taxes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Equivalent" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>The equivalent or converted fee amount associated to the item or service which does not include taxes. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TotalTax" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>The total fee taxes associated to the item or service purchased. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>The total fee amount associated to the service including taxes.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Filed" type="Amount.Basic.BankerSellingRate">
        <xsd:annotation>
          <xsd:documentation>The filed fee amount associated to the service.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="description" type="String.ItemDescription" use="optional">
      <xsd:annotation>
        <xsd:documentation>Description of the purchased item or service.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="quantity" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>The number of items purchased per PRAS code.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="coupon" type="Number.CouponSequence">
      <xsd:annotation>
        <xsd:documentation />
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Amount.Basic.BankerSellingRate">
    <xsd:complexContent mixed="false">
      <xsd:extension base="Amount.Basic">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="BankerSellingRate" type="CurrencyConversion.Details">
            <xsd:annotation>
              <xsd:documentation />
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Payment">
    <xsd:annotation>
      <xsd:documentation>Payment</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Base" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Base amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Tax" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Tax amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Total amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Remarks" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Remarks</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AccountCode" type="Identifier.PaymentAccount">
        <xsd:annotation>
          <xsd:documentation>Account code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeploymentId" type="Identifier.Deployment">
        <xsd:annotation>
          <xsd:documentation>A number generated by the supplier of virtual payments with details of the actual financial transaction</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:group ref="PaymentInstrument.CG" />
    </xsd:sequence>
    <xsd:attribute name="id" type="xsd:ID">
      <xsd:annotation>
        <xsd:documentation>Is a generated identifier assigned for a payment media for uniqueness in the
                    system.
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="type" type="Code.PaymentType" use="required">
      <xsd:annotation>
        <xsd:documentation>Same as payment instrument code</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sequence" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation>Defines the order of payments made.

                    Mapped to T2 database: TKT_DOC_PAYMNT.PAYMNT_SEQ_NBR
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="code" type="Code.FopVCR">
      <xsd:annotation>
        <xsd:documentation>Mapped to VCR_55_FPI

                    Applies only to VCR
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="missingPayAmtInd" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Set to 'Y' if Total Payment Amount is missing in the source record (i.e. VCR_55_FPA
                    is blank)
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="paymentConfirmation" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indicates that payment confirmation needs to be sent to Payment System after       successful transaction</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Payment.ED">
    <xsd:annotation>
      <xsd:documentation>Payment info for electronic documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Base" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Base amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Tax" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Tax amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="PaymentAmount.Basic">
        <xsd:annotation>
          <xsd:documentation>Total amount paid</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Remarks" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Additional information about the specified form of payment</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:group ref="PaymentInstrument.CG" />
    </xsd:sequence>
    <xsd:attribute name="type" type="Code.PaymentType" use="required">
      <xsd:annotation>
        <xsd:documentation>Same as payment instrument code</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="sequence" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation>Defines the order of payments made.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Related">
    <xsd:annotation>
      <xsd:documentation>Related documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="8" name="Conjunctive" type="RelatedDocument.Basic">
        <xsd:annotation>
          <xsd:documentation>Conjuctive ticket info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Original" type="RelatedDocument.Details">
        <xsd:annotation>
          <xsd:documentation>Original ticket information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="11" name="Exchange" type="RelatedDocument.Details">
        <xsd:annotation>
          <xsd:documentation>Exchanged ticket information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="11" name="Refund" type="RelatedDocument.Details">
        <xsd:annotation>
          <xsd:documentation>Refund information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="99" name="Linked" type="RelatedDocument.Abbreviated">
        <xsd:annotation>
          <xsd:documentation>Linked information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="6" name="Miscellaneous" type="RelatedDocument.Basic">
        <xsd:annotation>
          <xsd:documentation>Miscellaneous</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Related.Ticket.ED">
    <xsd:annotation>
      <xsd:documentation>Related tickets info</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="3" name="Conjunctive" type="RelatedDocument.Basic">
        <xsd:annotation>
          <xsd:documentation>Each ticket can accomodate 1-4 flight coupons. If the passenger's itinerary requires more than 4 flight coupons, additional 13 digit document numbers will be created by the system (in numeric sequence). When more than one document number is required, this is known as a conjuctive series. The maximum number of 13 digit document numbers in a conjuctive series is 4. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Original" type="RelatedDocument.Details.Original.TKT.ED">
        <xsd:annotation>
          <xsd:documentation>Details for the original ticket</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="11" name="Exchange" type="RelatedDocument.Details.ED">
        <xsd:annotation>
          <xsd:documentation>Details for the exchanged ticket</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Remark.ED">
    <xsd:annotation>
      <xsd:documentation>Remarks</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="999" name="Endorsements" type="Remark.DetailsWithType">
        <xsd:annotation>
          <xsd:documentation>Endorsements</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Manual" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>Additional remarks</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.Remark.EMD">
    <xsd:annotation>
      <xsd:documentation>Remarks</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Endorsements" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>Endorsements
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Manual" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>Manual remarks</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="10" name="Passenger" type="Remark.DetailsWithHeader">
        <xsd:annotation>
          <xsd:documentation>Passengers</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="20" name="Other" type="Remark.DetailsWithType">
        <xsd:annotation>
          <xsd:documentation>Other remarks</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.ServiceCoupon.EMD.ED">
    <xsd:annotation>
      <xsd:documentation>Service coupon</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="MarketingProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>Code identifying the Marketing Carrier for each coupon as it was sold</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarketingFlightNumber" type="STL:FlightNumber">
        <xsd:annotation>
          <xsd:documentation>Marketing carrier's flight number for each coupon as it was sold
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OperatingProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>Code identifying the Operating Carrier  for each coupon as it was sold (only populated for codeshare flights)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ClassOfService" type="ClassOfService.Details">
        <xsd:annotation>
          <xsd:documentation>RBB code retruned by the airline for this flight coupon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StartLocation">
        <xsd:annotation>
          <xsd:documentation>The airport or city code identifying the start location of the flight or service                    </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="STL:CityCode">
              <xsd:attribute name="name" type="String.LocationName">
                <xsd:annotation>
                  <xsd:documentation>Start location city name</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="StartDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The flight departure date and time or the start date and time when the service is applied</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EndLocation">
        <xsd:annotation>
          <xsd:documentation>The airport or city code identifying the end location of the flight or service</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="STL:CityCode">
              <xsd:attribute name="name" type="String.LocationName">
                <xsd:annotation>
                  <xsd:documentation>End location city name</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="EndDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The flight arrival date and time or the end date and time when the service is applied</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NotValidBeforeDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The date before which travel or service for this segment is no longer applicable </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NotValidAfterDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The date after which travel or service for this segment is no longer applicable </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SettlementAuthorization" type="Identifier.TicketingSettlementAuthorization">
        <xsd:annotation>
          <xsd:documentation>A reference number generated by the Valicating carrier authorising settlement of an EMD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BookingStatus">
        <xsd:annotation>
          <xsd:documentation>Code associated to the status of the booked segment Eg: HI = fulfilled, HD = Payment required</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="Code.BookingStatus">
              <xsd:attribute name="name" type="String.Short">
                <xsd:annotation>
                  <xsd:documentation>Booking status</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CouponStatus" type="Code.ServiceCouponStatus">
        <xsd:annotation>
          <xsd:documentation>A 1 to 2 digit code returned by EDIFACT which corresponds to the 4 digit coupon status code seen in the green screen display. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="coupon" type="Number.CouponSequence" use="required">
      <xsd:annotation>
        <xsd:documentation>Coupon number 1 through 16</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="description" type="String.ItemDescription" use="optional">
      <xsd:annotation>
        <xsd:documentation>Description of the purchased item or service.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="quantity" type="xsd:int">
      <xsd:annotation>
        <xsd:documentation>The number of items purchased per PRAS code.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TicketingDocument.ServiceCoupon.Ticket.ED">
    <xsd:annotation>
      <xsd:documentation>Service coupon</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="MarketingProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>Code identifying the Marketing Carrier for each coupon as it was sold</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarketingFlightNumber" type="STL:FlightNumber">
        <xsd:annotation>
          <xsd:documentation>Marketing carrier's flight number for each coupon as it was sold</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OperatingProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>Code identifying the Operating Carrier  for each coupon as it was sold (only populated for codeshare flights)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ClassOfService" type="ClassOfService.Details">
        <xsd:annotation>
          <xsd:documentation>RBD code retruned by the airline for this flight coupon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FareBasis" type="Code.FareBasis">
        <xsd:annotation>
          <xsd:documentation>Code that identifies the fare basis used on this coupon</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TicketDesignator" type="Code.TicketFareDesignator">
        <xsd:annotation>
          <xsd:documentation>Code following fare basis code either publilshed with the fare or manually input by the agent</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StartLocation">
        <xsd:annotation>
          <xsd:documentation>The airport or city code identifying the start location of the flight or service</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="STL:CityCode">
              <xsd:attribute name="name" type="String.LocationName">
                <xsd:annotation>
                  <xsd:documentation>Start location city name</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="StartDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The flight departure date and time or the start date and time when the service is applied</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EndLocation">
        <xsd:annotation>
          <xsd:documentation>The airport or city code identifying the end location of the flight or service</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="STL:CityCode">
              <xsd:attribute name="name" type="String.LocationName">
                <xsd:annotation>
                  <xsd:documentation>End location city name</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="EndDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The flight arrival date and time or the end date and time when the service is applied</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NotValidBeforeDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The date before which travel cannot commence.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NotValidAfterDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>The date after which travel or service for this coupon must be completed</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SettlementAuthorization" type="Identifier.TicketingSettlementAuthorization">
        <xsd:annotation>
          <xsd:documentation>A reference number generated by the Validating carrier authorising settlement of a coupon, usually present only for exchange and void transaction</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BookingStatus">
        <xsd:annotation>
          <xsd:documentation>2 digit code associated to the status of the flight segment at the time of ticketing</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="Code.BookingStatus">
              <xsd:attribute name="name" type="String.Short">
                <xsd:annotation>
                  <xsd:documentation>Booking status</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="CurrentStatus" type="Code.ServiceCouponStatus">
        <xsd:annotation>
          <xsd:documentation>A 1 to 2 digit code returned by EDIFACT which corresponds to the 4 digit coupon status code seen in the green screen display.  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BagAllowance" type="Code.BaggageAllowance">
        <xsd:annotation>
          <xsd:documentation>The applicable baggage allowance</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="coupon" type="Number.CouponSequence" use="required">
      <xsd:annotation>
        <xsd:documentation>The number representing the order in which this coupon was used or is to be used.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="type" type="Code.SegmentType" use="optional">
      <xsd:annotation>
        <xsd:documentation>Code associated to the segment type

                    Possible values can be:

                    A - Arunk
                    O - Open
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
