<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:STL="http://www.sabre.com/ns/Ticketing/EDocStl" xmlns="http://www.sabre.com/ns/Ticketing/EDoc" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.sabre.com/ns/Ticketing/EDoc" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:import schemaLocation="STL_For_TKT_ElectronicDocumentServices_v.1.0.0.xsd" namespace="http://www.sabre.com/ns/Ticketing/EDocStl" />
  <xsd:annotation>
    <xsd:documentation>Created by Eugene Almenar on 05/18/2012</xsd:documentation>
    <xsd:documentation>from the NewModel package with namespace http://www.sabre.com/ns/Ticketing/EDoc
        </xsd:documentation>
  </xsd:annotation>
  <xsd:element name="GetElectronicDocumentRQ" type="GetElectronicDocumentRQ">
    <xsd:annotation>
      <xsd:documentation>A request to get information about an electronic ticket or EMD from the carrier database (similar to WETR/WEMD)</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="GetElectronicDocumentRS" type="GetElectronicDocumentRS">
    <xsd:annotation>
      <xsd:documentation>A response message received with information about an electronic ticket or EMD from the carrier database
            </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="Affinity.FrequentFlyer.ED">
    <xsd:annotation>
      <xsd:documentation>Frequent Flyer Information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Provider" type="Code.TicketingProviderCode">
        <xsd:annotation>
          <xsd:documentation>The 2 digit code that identifies the airline of the frequent flyer program
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Number" type="Identifier.Affinity">
        <xsd:annotation>
          <xsd:documentation>An individual airline identifier assigned to a traveler who is a member of the frequent flyer program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount">
    <xsd:annotation>
      <xsd:documentation>Amount</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="STL:_amount">
        <xsd:attribute name="currencyCode" type="Code.CurrencyType">
          <xsd:annotation>
            <xsd:documentation>3 digit currency code Eg: USD, EUR</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="decimalPlace" type="Number.DecimalPlace">
          <xsd:annotation>
            <xsd:documentation>Number of places for decimal. Value can be 0, 2 or 3.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Basic">
    <xsd:annotation>
      <xsd:documentation>Amount with basic parameters</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>Actual Amount with currency code and decimal place.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Text" type="String.Short">
        <xsd:annotation>
          <xsd:documentation>Free text</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApplyCreditInd" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Indicates credit amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount.Commission">
    <xsd:annotation>
      <xsd:documentation>Amount related to Commission</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="Amount.Rate" />
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Commission.Agency">
    <xsd:annotation>
      <xsd:documentation>Amount related to Commission</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Commission" type="Amount.Commission">
        <xsd:annotation>
          <xsd:documentation>Commission details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount.Commission.Category35">
    <xsd:annotation>
      <xsd:documentation>Amount related to CAT35</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Commission" type="Amount.Commission">
        <xsd:annotation>
          <xsd:documentation>Commission details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MarkUp" type="Amount.Commission">
        <xsd:annotation>
          <xsd:documentation>Markup amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount.Commission.ED">
    <xsd:annotation>
      <xsd:documentation>Amount related to commision for electronic document</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="Amount.Rate">
        <xsd:attribute name="provider" type="Code.TicketingProviderCode">
          <xsd:annotation>
            <xsd:documentation>Provider info</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Details">
    <xsd:annotation>
      <xsd:documentation> Details of amounts</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>Amount information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Text" type="String.Short">
        <xsd:annotation>
          <xsd:documentation>Free text</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApplyCreditInd" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>If "N", this indicates a positive amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ValueCode" type="Code.AmountValue">
        <xsd:annotation>
          <xsd:documentation>Value code</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount.Details.CurrencyConversion">
    <xsd:annotation>
      <xsd:documentation>Currency conversion amounts</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="Amount.Details">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="CurrencyConversion" type="TicketingDocument.CurrencyConversion">
            <xsd:annotation>
              <xsd:documentation>Currency Conversion</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Details.ED">
    <xsd:annotation>
      <xsd:documentation> Details of amounts for electronic documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>Amount info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Text" type="String.Short">
        <xsd:annotation>
          <xsd:documentation>Free text</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApplyCreditInd" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>If the Indicator 'N' then the amounts are assumed to be Positive.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amount.Details.RateofExchange">
    <xsd:annotation>
      <xsd:documentation>Rate of exchange amounts</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="Amount.Details">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="RateOfExchange" type="CurrencyConversion.Details">
            <xsd:annotation>
              <xsd:documentation>Rate of exchange</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="Amount.Rate">
    <xsd:annotation>
      <xsd:documentation>Rate - needs more text</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>Amount info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PercentageRate" type="STL:Percentage">
        <xsd:annotation>
          <xsd:documentation>Percentage rate</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Text" type="String.Short">
        <xsd:annotation>
          <xsd:documentation>Free text</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amounts.Commission">
    <xsd:annotation>
      <xsd:documentation>Commission Amounts</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Category35" type="Amount.Commission.Category35">
        <xsd:annotation>
          <xsd:documentation>Commission type associated to automated commission rules
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Agency" type="Amount.Commission.Agency">
        <xsd:annotation>
          <xsd:documentation>Agency Managed Commission
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Manual" type="Amount.Commission">
        <xsd:annotation>
          <xsd:documentation>Manual commission
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amounts.New">
    <xsd:annotation>
      <xsd:documentation>Amounts</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Base" type="Amount.Details">
        <xsd:annotation>
          <xsd:documentation>Base amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Equivalent" type="Amount.Details.CurrencyConversion">
        <xsd:annotation>
          <xsd:documentation>Equivalent amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TotalTax" type="Amount.Details">
        <xsd:annotation>
          <xsd:documentation>Total Fee Charge Tax -- Refers to unpaid tax</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="Amount.Details.RateofExchange">
        <xsd:annotation>
          <xsd:documentation>Total amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Net" type="Amount.Details">
        <xsd:annotation>
          <xsd:documentation>Net amount</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="Tax" type="Tax.Details">
        <xsd:annotation>
          <xsd:documentation>This contains taxes that exist in the tax boxes 1, 2, 3.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amounts.New.ED">
    <xsd:annotation>
      <xsd:documentation>Amounts defined for electronic document</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Base" type="Amount.Details.ED">
        <xsd:annotation>
          <xsd:documentation>Base amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Equivalent" type="Amount.Details.ED">
        <xsd:annotation>
          <xsd:documentation>Equivalent amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Total" type="Amount.Details.ED">
        <xsd:annotation>
          <xsd:documentation>Total amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Amounts.RelatedDocument">
    <xsd:annotation>
      <xsd:documentation>Amounts associated with related documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Original" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Original amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Base" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Base amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Equivalent" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Equivalent amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TotalTax" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Total Taxes</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Total" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Total amounts</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ClassOfService.Details">
    <xsd:annotation>
      <xsd:documentation>Class of service</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="Code.ClassOfService">
        <xsd:attribute name="name" type="String.Short">
          <xsd:annotation>
            <xsd:documentation>Cl;ass of service</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Code.Approval">
    <xsd:annotation>
      <xsd:documentation>Approval code</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="Identifier.Approval">
        <xsd:attribute name="source" type="Code.ApprovalSource">
          <xsd:annotation>
            <xsd:documentation>Source of approval code Eg: "S"</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="type" type="Code.ApprovalType">
          <xsd:annotation>
            <xsd:documentation>1 digit value that indicates the means by which the approval code was obtained by the payment process. Eg: S, Z</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="CurrencyConversion.Details">
    <xsd:annotation>
      <xsd:documentation>Currency conversion</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:decimal">
        <xsd:attribute name="decimalPlace" type="Number.DecimalPlace">
          <xsd:annotation>
            <xsd:documentation>deimal place for the rate</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Code.BaggageAllowance">
    <xsd:attribute name="code" use="required">
      <xsd:annotation>
        <xsd:documentation>
K - Kilos
L- Pounds
PC - Pices
NIL - No free baggage allowance
10K - Infant with no seat
					</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="K" />
          <xsd:enumeration value="L" />
          <xsd:enumeration value="PC" />
          <xsd:enumeration value="NIL" />
          <xsd:enumeration value="10K" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="amount">
      <xsd:simpleType>
        <xsd:restriction base="xsd:int">
          <xsd:minInclusive value="0" />
          <xsd:maxInclusive value="99" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Customer.Traveler.Details.ED">
    <xsd:annotation>
      <xsd:documentation>Traveler information for electronic documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Name" type="ProperName.29Character">
        <xsd:annotation>
          <xsd:documentation>The customer's full name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FirstName" type="ProperName.29Character">
        <xsd:annotation>
          <xsd:documentation>The customer's first name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LastName" type="ProperName.29Character">
        <xsd:annotation>
          <xsd:documentation>The customer's last name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExternalNumber" type="Number.TicketingExternalCustomer">
        <xsd:annotation>
          <xsd:documentation>Free text following a * in the name entry input by the user during PNR creation. Also known as a "name reference" or MAN number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DateRange">
    <xsd:annotation>
      <xsd:documentation>date range</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="afterDate" type="xsd:date" use="required">
          <xsd:annotation>
            <xsd:documentation>After date</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="beforeDate" type="xsd:date" use="required">
          <xsd:annotation>
            <xsd:documentation>Before Date</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Diagnostic">
    <xsd:annotation>
      <xsd:documentation>Used for passing diagnostic information as text</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DataText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Diagnostic data </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DataXML">
        <xsd:annotation>
          <xsd:documentation>Diagnostic XML</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##any" processContents="lax" />
          </xsd:sequence>
          <xsd:anyAttribute namespace="##any" processContents="lax">
            <xsd:annotation>
              <xsd:documentation>
                                Prose in the spec does not specify that attributes are allowed on the DataXML element
                            </xsd:documentation>
            </xsd:annotation>
          </xsd:anyAttribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="type" type="STL:StringLength1to128" use="required">
      <xsd:annotation>
        <xsd:documentation>Type of data</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="DynamicCurrencyConversion">
    <xsd:annotation>
      <xsd:documentation>Dynamic Currency conversion</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="Amount">
        <xsd:annotation>
          <xsd:documentation>The payment amount in credit card currency with corresponding amount currency and
                        decimal places
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Text" type="STL:StringLength1to15">
        <xsd:annotation>
          <xsd:documentation>The payment amount in credit card currency in text form</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Rate" type="CurrencyConversion.Details">
        <xsd:annotation>
          <xsd:documentation>Rate of exchange between payment and credit card currency with corresponding
                        decimal places
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ElectronicDocument.SearchParameters">
    <xsd:annotation>
      <xsd:documentation>Search parameters for electronic document display</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DocumentNumber" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>Electronic document number (13 digit Ticket or EMD) dispensed and assigned by the system upon issuance; this is the primary document number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DocumentType" type="Code.DocumentType">
        <xsd:annotation>
          <xsd:documentation>Electronic document TYPE    TKT- Electronic Ticket    EMD - Electronic Misc Document 
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LineNumber" type="Number.LineNumber">
        <xsd:annotation>
          <xsd:documentation>Ticket line number from *T field in the PNR
                       
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CCView" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Indicates whether the CC infomration shoul dbe unmasked in the response</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="resultType" type="Code.GetTicketingResultType">
      <xsd:annotation>
        <xsd:documentation>Two types of requests are supported                         W - WETR*/WEMD* display H - WETR*/H display</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="FareCalculation.Details">
    <xsd:annotation>
      <xsd:documentation>Fare calculation</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="String.Text" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="GetElectronicDocumentRQ">
    <xsd:annotation>
      <xsd:documentation>A request message to get information about an electronic ticket or EMD</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="STL:STL_Header.RQ" />
      <xsd:element ref="STL:POS" />
      <xsd:element name="SearchParameters" type="ElectronicDocument.SearchParameters">
        <xsd:annotation>
          <xsd:documentation>Search parameters</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="5000" name="Diagnostic" type="Diagnostic">
        <xsd:annotation>
          <xsd:documentation>Internal use ONLY</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attributeGroup ref="TicketingDocumentStdAttributes" />
    <xsd:attribute name="requestType" type="Code.GetTicketingRequestType">
      <xsd:annotation>
        <xsd:documentation>request Type  W - WETR/WEMD request  H - WETRH request</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="GetElectronicDocumentRS">
    <xsd:annotation>
      <xsd:documentation>A response message received with information about an electronic ticket or EMD
            </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="STL:STL_Header.RS" />
      <xsd:element name="Agent" type="Ticketing.Agent.ED">
        <xsd:annotation>
          <xsd:documentation>Agent details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TransactionInfo" type="Ticketing.TransactionInfo.ED">
        <xsd:annotation>
          <xsd:documentation>Transactional details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:group minOccurs="0" ref="GetEDDisplayResultsChoice.CG" />
      <xsd:element minOccurs="0" maxOccurs="5000" name="Diagnostic" type="Diagnostic">
        <xsd:annotation>
          <xsd:documentation>Diagnostic information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attributeGroup ref="TicketingDocumentStdAttributes" />
  </xsd:complexType>
  <xsd:complexType name="Identifier.PaymentAccount">
    <xsd:annotation>
      <xsd:documentation>Payment account</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="Identifier.Account">
        <xsd:attribute name="type" type="Code.VirtualPayment">
          <xsd:annotation>
            <xsd:documentation>Payment account type</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Identifier.Provider">
    <xsd:annotation>
      <xsd:documentation>Provider</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="Code.TicketingProviderCode">
        <xsd:attribute name="number" type="Identifier.TicketingProviderAccounting">
          <xsd:annotation>
            <xsd:documentation>Provider number</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="checkDigit" type="Number.CheckDigit">
          <xsd:annotation>
            <xsd:documentation>Single numeric digit used internally to authenticate the electronic ticket number</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="name" type="String.ProviderName">
          <xsd:annotation>
            <xsd:documentation>reserved for future use</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Identifier.Reservation">
    <xsd:annotation>
      <xsd:documentation>Reservation information</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="PNR_Locator">
        <xsd:attribute name="provider" type="Code.TicketingProviderCode">
          <xsd:annotation>
            <xsd:documentation>Provider</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="purgeDate" type="xsd:dateTime">
          <xsd:annotation>
            <xsd:documentation>Purge Date</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="createDate" type="xsd:dateTime">
          <xsd:annotation>
            <xsd:documentation>PNR creation date</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="Miscellaneous.Indicators">
    <xsd:annotation>
      <xsd:documentation>Miscellaneous indicators</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="feeOverride" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A boolean (Y/N) indicator which determines whether the fee/taxes that should have been guaranteed have been altered by the agent</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="consumed" type="Code.IATAIndicators">
      <xsd:annotation>
        <xsd:documentation>A boolean (Y/N) indicator which determines whether the service was consumed at the time of issuance or not
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="taxExempt" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A boolean (Y/N) indicator which determines whether all taxes for this service/item are tax exempt or not
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Number.AssociatedDocument">
    <xsd:annotation>
      <xsd:documentation>Associated document number</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="STL:StringLength1to15">
        <xsd:attribute name="associationType" type="Code.AssociationType">
          <xsd:annotation>
            <xsd:documentation>Association type</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="couponNumber" type="Number.CouponSequence">
          <xsd:annotation>
            <xsd:documentation>Coupon number</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="checkDigit" type="Number.CheckDigit">
          <xsd:annotation>
            <xsd:documentation>check digit</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="OptionalService">
    <xsd:annotation>
      <xsd:documentation>Optional service</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="FeeProvider" type="Identifier.Provider">
        <xsd:annotation>
          <xsd:documentation>Fee provider </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Indicators" type="Miscellaneous.Indicators">
        <xsd:annotation>
          <xsd:documentation>Indicators </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PresentTo" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>The company or airline who is presented with a service coupon by the customer</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PresentAt" type="Remark.Details">
        <xsd:annotation>
          <xsd:documentation>The place where the company or airline is located</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="group" type="Code.FeeGroup" use="optional">
      <xsd:annotation>
        <xsd:documentation>Reason for issuance code (RFIC)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="groupDescription" type="String.Text" use="optional">
      <xsd:annotation>
        <xsd:documentation>Group description</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="reason" type="Code.ItemReasonForIssuance" use="required">
      <xsd:annotation>
        <xsd:documentation>Reason for issuance sub-code (RFISC).   </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ssr" type="Code.SSR">
      <xsd:annotation>
        <xsd:documentation>Special service request (SSR) code in the PNR associated to the item.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="filingSource" type="Code.FilingSource">
      <xsd:annotation>
        <xsd:documentation>Ancillary item filing source code </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="journeyType" type="Code.JourneyType">
      <xsd:annotation>
        <xsd:documentation>Needed for EMD. The value can be sector (S) or portion (P)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Payment.BankCard.ED">
    <xsd:annotation>
      <xsd:documentation>Bank card information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="MaskedCardNumber" type="Number.CreditCard">
        <xsd:annotation>
          <xsd:documentation>All numbers except the last 4 will be masked with "X" in the card number unless the user requested unmasking and has appropriate privileges to view it. Eg:XXXXXXXXXXXX0010 </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpireDate" type="Expire_Date">
        <xsd:annotation>
          <xsd:documentation>Expiration date of the card will be masked unless the user requsted unmasking and has appropriate privileges.    Format is MMDD. Masked expiration date will appear as XXXX.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApprovalCode" type="Code.Approval">
        <xsd:annotation>
          <xsd:documentation>6 digit alphanumeric value assigned by the merchant bank during the payment process.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExtendedPayment" type="Code.ExtendedPayment">
        <xsd:annotation>
          <xsd:documentation>Presence indicates extended payment feature when issuing a ticket. This only applies to American Express.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Details" type="String.Text">
        <xsd:annotation>
          <xsd:documentation>Free text</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Authorized" type="Amount.Basic">
        <xsd:annotation>
          <xsd:documentation>Authorized amount (CC)
</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AddressVerification" type="Code.AddressVerification">
        <xsd:annotation>
          <xsd:documentation>Address Verification (CC)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TransactionId" type="Identifier.Transaction">
        <xsd:annotation>
          <xsd:documentation>Credit card transaction information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="cardType" type="Code.OTA">
      <xsd:annotation>
        <xsd:documentation>2 digit CC code or Vendor code. Eg: VI, MC, AX</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Payment.BankCheck">
    <xsd:annotation>
      <xsd:documentation>Check information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RoutingNumber" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation>Represents the numeric code identifying the financial institution for a specific account.

                        Currently not used in Ticketing.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApprovalCode" type="Code.CheckApproval">
        <xsd:annotation>
          <xsd:documentation>A series of characters assigned by the applicable Bank authorization system to  confirm the approval of a sale transaction paid with check.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.Cash">
    <xsd:annotation>
      <xsd:documentation>Cash information</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute default="true" name="cashIndicator" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>If true, this indicates cash is being used.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Payment.Certficate">
    <xsd:annotation>
      <xsd:documentation>Certificate information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="STL:StringLength1to14">
        <xsd:annotation>
          <xsd:documentation>Cetificate number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.GovernmentTravel">
    <xsd:annotation>
      <xsd:documentation>Government travel information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="STL:StringLength1to13">
        <xsd:annotation>
          <xsd:documentation>Government travel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.Invoice">
    <xsd:annotation>
      <xsd:documentation>Invoice payment</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="STL:StringLength1to14">
        <xsd:annotation>
          <xsd:documentation>Invoice</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.Miscellaneous">
    <xsd:annotation>
      <xsd:documentation>Miscellaneous payment</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute default="true" name="miscIndicator" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>If true, this indicates miscellaneous (unknown) payment is being used. </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="Payment.Other">
    <xsd:annotation>
      <xsd:documentation>Other forms of payment</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Details" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Other form of payment</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.PrepaidTicketAdvise">
    <xsd:annotation>
      <xsd:documentation>Prepaid ticket advice</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="STL:StringLength1to14">
        <xsd:annotation>
          <xsd:documentation>Prepaid ticket advice number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.RevenueAccounting">
    <xsd:annotation>
      <xsd:documentation>Revenue accounting</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="STL:StringLength1to10">
        <xsd:annotation>
          <xsd:documentation>Revenue accounting number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.TravelDocument">
    <xsd:annotation>
      <xsd:documentation>Travel document</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Number" type="xsd:decimal">
        <xsd:annotation>
          <xsd:documentation>An identifier for issued document.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Expiration" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The last day an unused travel document may be used as a form of payment
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Issue" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The date the document was issued</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Amount" type="STL:Amount">
        <xsd:annotation>
          <xsd:documentation>The amount that is available to be used as a form of payment for other
                        services.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FormOfPayment" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>The form of payment is not in the Sabre ticketing system. It is a manually
                        entered field used to track the form of payment for the document.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PlaceOfIssuance" type="String.ProperName">
        <xsd:annotation>
          <xsd:documentation>Place of issuance</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ServiceCoupon" type="String.ServiceCoupon">
        <xsd:annotation>
          <xsd:documentation>Number of the ticket document coupon used as a form a payment.
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="Payment.Voucher">
    <xsd:annotation>
      <xsd:documentation>Voucher</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ExpireDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Expiration date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SeriesCode" type="Code.VoucherSeries">
        <xsd:annotation>
          <xsd:documentation>Not currently used</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PaymentAmount">
    <xsd:annotation>
      <xsd:documentation>Payment amount</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="STL:_amount">
        <xsd:attribute name="currencyCode" type="Code.CurrencyType">
          <xsd:annotation>
            <xsd:documentation>3 digit currency code Eg: USD, EUR</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="decimalPlace" type="Number.DecimalPlace">
          <xsd:annotation>
            <xsd:documentation>Number of places for decimal. Value can be 0, 2 or 3.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="PaymentAmount.Basic">
    <xsd:annotation>
      <xsd:documentation>Basic payment amount</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Amount" type="PaymentAmount">
        <xsd:annotation>
          <xsd:documentation>Actual Amount with currency code and decimal place. Eg: USD 25.55  </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ApplyCreditInd" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>If the Indicator is false hen the amounts are assumed to be positive.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="RelatedDocument.Abbreviated">
    <xsd:annotation>
      <xsd:documentation>Abbreviated info for related doucments</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Number" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>Document number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CheckDigit" type="Number.CheckDigit">
        <xsd:annotation>
          <xsd:documentation>check digit</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Issue date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="16" name="AssociatedTicketNumber" type="Number.AssociatedDocument">
        <xsd:annotation>
          <xsd:documentation>Asscoaited Ticket number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="sequence" type="xsd:integer">
      <xsd:annotation>
        <xsd:documentation>Orde of documents presented</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="type" type="Code.DocumentType">
      <xsd:annotation>
        <xsd:documentation>Document type</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="electronic" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A Y/N indicator which identifies whether the document is produced electronically.                  Default is "N", which means PAPER</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="status" type="Code.DocumentStatus">
      <xsd:annotation>
        <xsd:documentation>Current status of the ticketing document

             </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="primary" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>A Y/N indicator which identifies whether the linked document is primary or not.

                    Default is "N", which means not primary document.
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="RelatedDocument.Basic">
    <xsd:annotation>
      <xsd:documentation>Basic info for related documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Number" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>The 13 digit electronic document number dispensed and assigned by the system upon issuance.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="sequence" type="xsd:integer">
      <xsd:annotation>
        <xsd:documentation>The order in which the tickets were issued</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="RelatedDocument.Details">
    <xsd:annotation>
      <xsd:documentation>Details for related documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Number" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>Original document info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Payment" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Payment info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CheckDigit" type="Number.CheckDigit">
        <xsd:annotation>
          <xsd:documentation>Check digit</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Coupon" type="String.ServiceCoupon">
        <xsd:annotation>
          <xsd:documentation>Coupon </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Issue date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Issue time</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueCity" type="Identifier.Location">
        <xsd:annotation>
          <xsd:documentation>Issue city</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IataNumber" type="Identifier.Industry">
        <xsd:annotation>
          <xsd:documentation>Iata number</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Amounts" type="Amounts.RelatedDocument">
        <xsd:annotation>
          <xsd:documentation>Amounts for related documents</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Remark" type="String.TicketingRemark">
        <xsd:annotation>
          <xsd:documentation>Issued in exchange for data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TransactionId" type="Identifier.Transaction">
        <xsd:annotation>
          <xsd:documentation>Related Document credit card transaction information</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DynamicCurrencyConversion" type="DynamicCurrencyConversion">
        <xsd:annotation>
          <xsd:documentation>Details related to payment amount conversion to the credit card currency code for related document</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="OriginalPayment" type="TicketingDocument.Payment.ED">
        <xsd:annotation>
          <xsd:documentation>Original form of payment</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="sequence" type="xsd:integer">
      <xsd:annotation>
        <xsd:documentation>Document sequence number</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="notInVcr" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Set to 'Y' if data is missing in the source record
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="paymentConfirmation" type="xsd:boolean">
      <xsd:annotation>
        <xsd:documentation>Indicates that payment confirmation needs to be sent after
                    successful transaction</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="RelatedDocument.Details.ED">
    <xsd:annotation>
      <xsd:documentation>Details for related documents, specific to electronic documents</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Number" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>The 13 digit electronic document number dispensed and assigned by the system upon issuance.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Payment" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Payment Remarks</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Coupon" type="String.ServiceCoupon">
        <xsd:annotation>
          <xsd:documentation>Coupon number </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueDate" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Date electronic ticket was issued</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueTime" type="xsd:time">
        <xsd:annotation>
          <xsd:documentation>Time electronic ticket was issued</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueCity" type="Identifier.Location">
        <xsd:annotation>
          <xsd:documentation>City code of travel agency as per TJR where the electronic ticket electronic ticket was issued</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IataNumber" type="Identifier.Industry">
        <xsd:annotation>
          <xsd:documentation>IATA number for the agency per TJR where the electronic ticket is issued</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="OriginalPayment" type="TicketingDocument.Payment.ED">
        <xsd:annotation>
          <xsd:documentation>Original form of payment</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="sequence" type="xsd:integer">
      <xsd:annotation>
        <xsd:documentation>The sequence in which the documents were issued</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="RelatedDocument.Details.Original.TKT">
    <xsd:annotation>
      <xsd:documentation>Details for the original ticket</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Number" type="Identifier.TicketingDocument">
        <xsd:annotation>
          <xsd:documentation>Mapped to T2 database:
                        TKT_REL_DOC.REL_DOC_ID
                    </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Payment" type="String.FormOfPayment">
        <xsd:annotation>
          <xsd:documentation>Mapped to T2 database: TKT_REL_DOC.REL_DOC_PAYMNT_RMK_TXT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CheckDigit" type="Number.CheckDigit">
        <xsd:annotation>
          <xsd:documentation>Mapped to T2 database: TKT_REL_DOC.REL_DOC_CHK_DIGT_NBR</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
