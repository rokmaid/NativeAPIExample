        </xsd:annotation>
        <xsd:complexType>
          <xsd:group ref="ElectronicDocumentTypeChoice.CG" />
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="DocumentHistoryDisplay">
        <xsd:annotation>
          <xsd:documentation>Reserved for future use</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:choice>
            <xsd:element minOccurs="0" name="History" type="TicketingDocument.History.ED">
              <xsd:annotation>
                <xsd:documentation>Reserved for future use</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:group>
  <xsd:group name="PaymentInstrument.CG">
    <xsd:annotation>
      <xsd:documentation>Payment instrument choice</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="Card" type="Payment.BankCard.ED">
        <xsd:annotation>
          <xsd:documentation>Credit card info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Cash" type="Payment.Cash">
        <xsd:annotation>
          <xsd:documentation>Cash payment info</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Check" type="Payment.BankCheck">
        <xsd:annotation>
          <xsd:documentation>Check Details</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GovernmentTravel" type="Payment.GovernmentTravel">
        <xsd:annotation>
          <xsd:documentation>Government travel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Invoice" type="Payment.Invoice">
        <xsd:annotation>
          <xsd:documentation>Invoice</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Miscellaneous" type="Payment.Miscellaneous">
        <xsd:annotation>
          <xsd:documentation>Will be returned in response as free text. Eg: MSCADEPOSIT</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Other" type="Payment.Other">
        <xsd:annotation>
          <xsd:documentation>Other payment</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:group>
  <xsd:attributeGroup name="TicketingDocumentStdAttributes">
    <xsd:annotation>
      <xsd:documentation>Standard attributes</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="Version" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Version of the service the service consumer wishes to use.
                    NOTE: In order for SWSO to be able to capture the version of the service being used in their
                    logs/statistics,
                    this attribute needs to be named Version (with a capital letter - which is against our naming
                    convention).
                </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
</xsd:schema>