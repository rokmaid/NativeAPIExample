                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="LCC" type="ToggleType" use="optional">
                    <xs:annotation>
                      <xs:documentation>Toggle processing of Low Cost Carriers data source.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="NDC" type="ToggleType" use="optional">
                    <xs:annotation>
                      <xs:documentation>Toggle processing of New Distribution Capability data source.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:element minOccurs="0" maxOccurs="1" name="VerificationItinCallLogic">
              <xs:complexType>
                <xs:attribute name="Value" type="VerificationItinCallLogicType">
                  <xs:annotation>
                    <xs:documentation>Define how the current itinerary is priced.</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="AlwaysCheckAvailability" type="xs:boolean" use="optional">
                  <xs:annotation>
                    <xs:documentation>Check availability for repriced itin.</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ClassOfService" type="ClassOfServiceElemType">
              <xs:annotation>
