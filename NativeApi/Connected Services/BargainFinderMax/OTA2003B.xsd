            <xs:element minOccurs="0" name="SellingLevels">
              <xs:complexType>
                <xs:choice>
                  <xs:element name="SellingLevelRules">
                    <xs:complexType>
                      <xs:attribute name="Ignore" type="xs:boolean" use="required">
