            <xs:element minOccurs="0" name="RetailerRules" type="RetailerRulesType" />
            <xs:element minOccurs="0" name="ForceKeywordRestrictedContent">
              <xs:annotation>
                <xs:documentation>Fail request if keyword validation fails</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:attribute name="Value" type="xs:boolean" use="required" />
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="FlightRestrictions">
              <xs:complexType>
                <xs:attribute name="AvoidLastFlight" type="xs:boolean" use="optional">
                  <xs:annotation>
                    <xs:documentation>Avoid last flight of the day.</xs:documentation>
