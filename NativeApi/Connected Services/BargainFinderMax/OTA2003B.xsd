              </xs:complexType>
            </xs:element>
            <xs:choice>
              <xs:element minOccurs="0" maxOccurs="1" name="LowCostCarriersProcessing">
                <xs:complexType>
                  <xs:attribute name="Disable" type="xs:boolean" use="optional">
